    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.4",
    "eslint-plugin-unicorn": "^60.0.0",
    "fzstd": "^0.1.1",
    "get-tsconfig": "^4.10.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "inversify": "^7.7.0",
    "js-yaml": "^4.1.0",
    "lint-staged": "^16.1.4",
    "minimist": "^1.2.8",
    "msw": "^2.10.4",
